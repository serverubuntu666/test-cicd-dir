pipeline {
    agent { label "DEV"}


    stages {
        stage('build phase') {
            steps {
                echo "this is a build phase."
            }
        }

        stage('testing phase') {
            steps {
                echo "this is testing phase...."
            }
        }

        stage('deploying phase') {
            steps {
                echo "this is deploying phase...."
            }
        }
    }
}